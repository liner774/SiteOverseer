@model SiteOverseer.Models.Facility

@{
    ViewData["Title"] = "Facility";
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-6">
                <h1>@ViewData["Title"]</h1>
            </div>
            <div class="col-6">
                <div class="breadcrumb float-right active">
                    <a asp-action="Index" class="btn btn-primary">Back to List <i class="fas fa-back"></i></a>

                </div>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card card-gray">
            <div class="card-header">
                <h3 class="card-title">
                    Are you sure you want to delete this?
                </h3>
            </div>
            <div class="card-body">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilCde)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilCde)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilNme)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilNme)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Address)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Address)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Township)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Township)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CityId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CityId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FciltypId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FciltypId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Contractval)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Contractval)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ApproveDte)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ApproveDte)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilstartDte)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilstartDte)
                    </dd>
                   
                </dl>
            </div>

            <div class="card-footer">
                <form asp-action="Delete">
                    <input type="hidden" asp-for="FcilId" />
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>
            </div>
        </div>
    </div>
</section>




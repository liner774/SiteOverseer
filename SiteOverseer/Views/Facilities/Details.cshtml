@model SiteOverseer.Models.Facility

@{
    ViewData["Title"] = "Facility";
}



<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-6">
                <h1>@ViewData["Title"]</h1>
            </div>
            <div class="col-6">
                <div class="breadcrumb float-right active">
                    <a asp-action="Index" class="btn btn-primary">Back to List <i class="fas fa-back"></i></a>

                </div>
            </div>
        </div>
    </div>
</section>


<section class="content">
    <div class="container-fluid">
        <div class="card card-gray">
            <div class="card-header">
                <h3 class="card-title">@ViewData["Title"] Details </h3>
            </div>
            <div class="card-body">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilCde)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilCde)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilNme)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilNme)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Address)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Address)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Township)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Township)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CityId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CityName)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FciltypId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FciltypCde)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Contractval)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Contractval)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ApproveDte)
                    </dt>
                    <dd class="col-sm-10">
                        @Convert.ToDateTime(Model.ApproveDte).ToString("dd MMM yyyy")
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilstartDte)
                    </dt>
                    <dd class="col-sm-10">
                        @Convert.ToDateTime(Model.FcilstartDte).ToString("dd MMM yyyy")
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Company)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Company)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.RevdTetime)
                    </dt>
                    <dd class="col-sm-10">
                        @Convert.ToDateTime(Model.RevdTetime).ToString("dd MMM yyy hh:mm:ss tt")
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.User)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.User)
                    </dd>
                    
                </dl>
            </div>
            
        </div>
    </div>
</section>





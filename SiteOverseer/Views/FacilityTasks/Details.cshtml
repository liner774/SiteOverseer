@model SiteOverseer.Models.FacilityTask

@{
    ViewData["Title"] = "Facility Task";
}




<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-6">
                <h1>@ViewData["Title"]</h1>
            </div>
            <div class="col-6">
                <div class="breadcrumb float-right active">
                    <a asp-action="Index" class="btn btn-primary">Back to List <i class="fas fa-back"></i></a>

                </div>
            </div>
        </div>
    </div>
</section>


<section class="content">
    <div class="container-fluid">
        <div class="card card-gray">
            <div class="card-header">
                <h3 class="card-title">@ViewData["Title"] Details </h3>
            </div>
            <div class="card-body">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FciltskId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FciltskId)
                    </dd>

                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FcilNme)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FcilNme)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.WbsdId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.WbsdId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Budget)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Budget)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CntorNme)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CntorNme)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.SelectionTyp)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.SelectionTyp)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.WorkstartDte)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.WorkstartDte)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.WorkendDte)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.WorkendDte)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.AwardedValue)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.AwardedValue)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProgpayId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ProgpayId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.AllowSubmitExpense)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.AllowSubmitExpense)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.TaskCompleteFlg)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.TaskCompleteFlg)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Remark)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Remark)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CmpyId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CmpyId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.RevdTetime)
                    </dt>
                    <dd class="col-sm-10">
                        @Convert.ToDateTime(Model.RevdTetime).ToString("dd MMM yyy hh:mm:ss tt")
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.UserId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.UserId)
                    </dd>
                    
                </dl>
            </div>
            
        </div>
    </div>
</section>




